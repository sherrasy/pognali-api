generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextSearch"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Country {
  id          Int      @id @default(autoincrement()) @map("country_id")
  name        String
  flag        String
  region      String
  letter      String
  isIsland    Boolean
  createdDate DateTime @default(now())

  @@map("countries")
}
